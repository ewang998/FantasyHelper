<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#006699"><![CDATA[
        tools:context="ewang.fantasyhelper.MainActivity">

    ]]>
    <EditText
            android:layout_width="384dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="@string/welcometitle"
            android:ems="10"
            android:id="@+id/editText2"
            android:textColor="#FFFFFF" tools:layout_constraintTop_creator="1" android:layout_marginStart="16dp"
            android:layout_marginTop="35dp" tools:layout_constraintLeft_creator="1"
            app:layout_constraintLeft_toLeftOf="parent" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginLeft="-51dp"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textEmailAddress"
            android:ems="10"
            android:width="190dp"
            android:id="@+id/editText3" tools:layout_constraintTop_creator="1"
            app:layout_constraintRight_toRightOf="parent" app:layout_constraintTop_toBottomOf="@+id/textView3"
            app:layout_constraintLeft_toLeftOf="parent"
    />
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPassword"
            android:ems="10"
            android:width="190dp"
            android:id="@+id/editText4" tools:layout_constraintTop_creator="1" tools:layout_constraintRight_creator="1"
            app:layout_constraintRight_toRightOf="@+id/editText3" android:layout_marginTop="83dp"
            app:layout_constraintTop_toBottomOf="@+id/editText3" tools:layout_constraintLeft_creator="1"
            app:layout_constraintLeft_toLeftOf="@+id/editText3" app:layout_constraintHorizontal_bias="1.0"/>
    <Button
            android:text="@string/log_in"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/button" tools:layout_constraintTop_creator="1" tools:layout_constraintRight_creator="1"
            android:layout_marginEnd="38dp" app:layout_constraintRight_toRightOf="@+id/editText4"
            android:layout_marginTop="55dp" app:layout_constraintTop_toBottomOf="@+id/editText4"
            android:layout_marginRight="38dp"/>
    <TextView
            android:text="E-Mail"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView3" tools:text="@string/Email"
            android:textSize="18sp" android:textColor="#FFFFFF" tools:layout_constraintTop_creator="1"
            android:layout_marginTop="53dp" app:layout_constraintTop_toBottomOf="@+id/editText2"
            tools:layout_constraintLeft_creator="1" app:layout_constraintLeft_toLeftOf="@+id/editText3"/>
    <TextView
            android:text="@string/passwordonlaunchpage"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView4" tools:text="Password" android:textSize="18sp"
            tools:layout_constraintBottom_creator="1"
            app:layout_constraintBottom_toTopOf="@+id/editText4" tools:layout_constraintLeft_creator="1"
            app:layout_constraintLeft_toLeftOf="@+id/editText4" android:layout_marginLeft="-41dp"
            android:layout_marginBottom="0dp" android:textColor="#FFFFFF"/>
    <android.support.constraint.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                          android:id="@+id/guideline" app:layout_constraintGuide_begin="20dp"
                                          android:orientation="horizontal"/>
</android.support.constraint.ConstraintLayout>
